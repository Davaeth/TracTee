CREATE TABLE TimerEntity (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    time INTEGER NOT NULL,
    title TEXT
);

insertTimer:
INSERT INTO TimerEntity (time, title)
VALUES (?, ?);

getNewestTimer:
SELECT *
FROM TimerEntity
ORDER BY id DESC
LIMIT 1;

retrieveTimers:
SELECT *
FROM TimerEntity;

updateTimer:
UPDATE TimerEntity
SET time = ?, title = ?
WHERE id LIKE ?;